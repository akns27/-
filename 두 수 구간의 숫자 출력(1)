#include <stdio.h>

int main() 
{
    int a, b;
    printf("***두 수 구간의 숫자 출력하기***\n");
    printf("두 수를 입력하시오: ");
    scanf("%d %d", &a, &b);
    
    if (a < b) {
        for (int i = a; i <= b; i++) {
            printf("%d ", i);
        }
    } else if (a > b) {
        for (int i = b; i <= a; i++) {
            printf("%d ", i);
        }
    } else {
        printf("%d", a);
    }

    return 0;
}
